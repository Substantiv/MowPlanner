<launch>
    <!-- Load World -->
    <!-- simple slope -->
    <arg name="world_name" default="$(find willand_description)/worlds/slope_world_20deg.world"/>
    <!-- complex lawn -->
    <!-- <arg name="world_name" default="$(find willand_description)/worlds/lawn_world.world"/> -->

    <!-- Launch Gazebo -->
    <include file="$(find gazebo_ros)/launch/empty_world.launch">
        <arg name="world_name" value="$(arg world_name)"/>
        <arg name="paused" value="false"/>
    </include>

    <!-- Spawn URDF Model -->
    <include file="$(find willand_description)/launch/spawn.launch"/>

    <!-- Spawn Controllers -->
    <include file="$(find willand_description)/launch/controller.launch"/>

    <!-- Create TF transforms from /map to /base_link -->
    <node name="map2baselink" pkg="willand_description" type="odom_process.py" output="screen"/>

    <!-- Launch Rviz -->
    <node name="rviz" pkg="rviz" type="rviz" args="-d $(find willand_description)/config/willand_orabmap_rviz.rviz" />

     <!-- Convert joint states to TF transforms for rviz -->
    <node name="robot_state_publisher" pkg="robot_state_publisher" type="robot_state_publisher"
        respawn="false" output="screen">
        <remap from="/joint_states" to="/willand/joint_states" />
    </node>

    <!-- <node name="contact_read_node" pkg="willand_description" type="contact_read_node" output="screen"/> -->

</launch>