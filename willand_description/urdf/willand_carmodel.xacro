<joint name="joint_front_left" type="continuous">
  <origin rpy="0 0 0" xyz="0.13189 0.1675 0.03018 "/>
  <axis rpy="0 0 0" xyz="0 1 0"/>
  <parent link="base_link"/>
  <child link="wheel_front_left_1"/>
